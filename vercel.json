{
    "relatedProjects": [
        "apps/api",
        "apps/frontend"
    ],
    "functions": {
        "api/*": {
            "maxDuration": 10,
            "memory": 1024,
            "runtime": "nodejs22.x"
        }
    },
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/apps/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/frontend/$1"
        }
    ]
}